{
  "name": "aircraft-tracker-3d",
  "version": "1.0.0",
  "type": "module",
  "description": "A 3D aircraft tracking system using Cesium.js and real-time IoT sensor data.",
  "main": "index.js",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "proxy": "node enhanced-proxy.js",
    "amqp-broker": "node amqp-broker.js",
    "amqp-proxy": "node amqp-proxy.js",
    "amqp-publisher": "node amqp-publisher.js",
    "dev:full": "concurrently \"npm run proxy\" \"npm run dev\"",
    "dev:amqp": "concurrently \"npm run amqp-broker\" \"npm run amqp-publisher\" \"npm run amqp-proxy\" \"npm run dev\"",
    "lint": "eslint ."
  },
    "dev:amqp": "concurrently \"npm run amqp-broker\" \"npm run amqp-proxy\" \"npm run dev\"",
    "lint": "eslint ."
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "nuxt": "^3.0.0",
    "rhea": "^3.0.4",
    "three": "^0.132.2",
    "vue": "^3.2.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.5.7",
    "@vue/compiler-sfc": "^3.2.0",
    "concurrently": "^7.6.0",
    "eslint": "^9.32.0"
  },
  "license": "MIT"
}
