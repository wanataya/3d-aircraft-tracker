# Aircraft Tracker 3D Configuration
# Copy this file to .env and update with your actual values

# AMQP Configuration (Keep these values private!)
AMQP_HOST=your.sensor.ip.address
AMQP_PORT=30003
AMQP_USERNAME=your_username
AMQP_PASSWORD=your_password
AMQP_QUEUE=aircraft-data
AMQP_EXCHANGE=aircraft

# TCP IoT Sensor Configuration (Required for production)
TCP_HOST=your.sensor.ip.address
TCP_PORT=30003

# WebSocket Proxy Configuration  
WS_PROXY_PORT=8080
WS_PROXY_HOST=localhost

# Protocol Selection: 'amqp' or 'tcp'
DEFAULT_PROTOCOL=amqp

# Application Configuration
NUXT_PUBLIC_APP_NAME=Aircraft Tracker 3D
NUXT_PUBLIC_APP_DESCRIPTION=A 3D aircraft tracking system
